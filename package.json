{
  "name": "keruwawa",
  "version": "0.0.0",
  "description": "Reports and graphs generated based on my time-sheet data.",
  "private": true,
  "main": "index.js",
  "bin": {
    "keruwawa-server": "dist/server.js"
  },
  "scripts": {
    "prebuild": "npm test",
    "build": "webpack",
    "postbuild": "sed -i '1i#!/usr/bin/env node' $npm_package_bin_keruwawa_server",
    "clean": "rm -fr dist/",
    "start": "nodemon $npm_package_bin_keruwawa_server -c config.json",
    "pretest": "eslint src/",
    "test": "exit 0;"
  },
  "keywords": [
    "time-sheet",
    "time-tracking",
    "reports"
  ],
  "author": "Sudaraka Wijesinghe <sudaraka@sudaraka.org> (https://sudaraka.org/)",
  "license": "BSD-2-Clause",
  "dependencies": {
    "deep-assign": "^2.0.0",
    "express": "^4.13.4",
    "mysql": "^2.11.1",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "@sudaraka/eslint-config-default": "^0.5.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^2.12.0",
    "nodemon": "^1.9.2",
    "webpack": "^1.13.1"
  }
}
